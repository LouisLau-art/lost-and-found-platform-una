{
  "name": "@una-ui/nuxt",
  "type": "module",
  "version": "1.0.0-alpha.12",
  "description": "Nuxt module for @una-ui",
  "author": "Phojie Rengel <phojrengel@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/phojie",
  "homepage": "https://github.com/una-ui/una-ui/tree/main/packages/nuxt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/una-ui/una-ui.git"
  },
  "imports": {
    "#build/una/*": "./.nuxt/una/*.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./una.config": {
      "import": "./dist/una.config.mjs"
    },
    "./runtime/*": "./dist/runtime/*",
    "./components/*": "./dist/runtime/components/*",
    "./composables/*": "./dist/runtime/composables/*",
    "./utils/*": {
      "types": "./dist/runtime/utils/*.d.ts",
      "import": "./dist/runtime/utils/*.js"
    }
  },
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@iconify/utils": "^2.3.0",
    "@nuxt/kit": "^3.19.2",
    "@nuxtjs/color-mode": "^3.5.2",
    "@tanstack/vue-table": "^8.21.3",
    "@unocss/core": "^66.5.4",
    "@unocss/nuxt": "^66.5.4",
    "@unocss/preset-attributify": "^66.5.4",
    "@unocss/preset-icons": "^66.5.4",
    "@unocss/reset": "^66.5.4",
    "@vee-validate/nuxt": "^4.15.1",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^12.8.2",
    "@vueuse/integrations": "^12.8.2",
    "@vueuse/nuxt": "^12.8.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ohash": "^1.1.6",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.4.0",
    "unocss": "^66.5.4",
    "unocss-preset-animations": "^1.3.0",
    "vaul-vue": "^0.4.1",
    "@una-ui/extractor-vue-script": "^1.0.0-alpha.12",
    "@una-ui/preset": "^1.0.0-alpha.12"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.74",
    "@iconify-json/radix-icons": "^1.2.5",
    "@iconify-json/tabler": "^1.2.23",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^3.19.2",
    "nuxt": "^3.19.2",
    "zod": "^3.25.76"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "nuxt-build-module build",
    "stub": "nuxt-build-module build --stub && pnpm nuxt-module-build prepare",
    "dev": "nuxi dev playground",
    "playground:build": "nuxi build playground",
    "playground:preview": "nuxi preview playground"
  }
}